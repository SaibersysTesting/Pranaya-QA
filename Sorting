package first;

public class Sorting 
{
 public static void main(String[] args)
{
	int[] a = {10,20,50,30};
	int i,j,temp;
	int size = a.length;
	System.out.println("sorted array is:");
	for(i=0;i<size;i++)
	{
		for(j=i;j<size-1;j++)
		{
			if(a[j]>a[j+1])
			{
				temp=a[j+1];
				a[j+1]=a[j];
			    a[j]=temp;
		    }
        }
		System.out.println(+a[i]);
     }
   }
}
